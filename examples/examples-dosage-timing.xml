<!-- "file": "examples-dosage-timing.xml" -->
<!-- Example use of the FHIR STU3 Dosage.timing -->
<!-- every 8 hours -->
<timing>
 <repeat>
  <frequency value="1"/>
  <period value="8"/>
  <periodUnit value="h"/>
 </repeat>
</timing>
<!-- 4 times a day -->
<timing>
 <repeat>
  <frequency value="4"/>
  <period value="1"/>
  <periodUnit value="d"/>
 </repeat>
</timing>
<!-- 2 to 4 times a day -->
<timing>
 <repeat>
  <frequency value="2"/>
  <frequencyMax value="4"/>
  <period value="1"/>
  <periodUnit value="d"/>
 </repeat>
</timing>
<!-- every 3 to 4 weeks -->
<timing>
 <repeat>
  <frequency value="1"/>
  <period value="3"/>
  <periodMax="4"/>
  <periodUnit value="wk"/>
 </repeat>
</timing>
<!-- 3 to 4 times every 1 to 2 weeks -->
<timing>
 <repeat>
  <frequency value="3"/>
  <frequencyMax value="4"/>
  <period value="1"/>
  <periodMax value="2"/>
  <periodUnit value="wk"/>
 </repeat>
</timing>
<!-- daily -->
<timing>
 <repeat>
  <period value="1"/>
  <periodUnit value="d"/>
 </repeat>
</timing>
<!-- twice -->
<timing>
 <repeat>
  <frequency value="2"/>
 </repeat>
</timing>